#!/usr/bin/env zsh
# Parses the return of a fzf multi-selection
local rawSelection="$1"

# Stop early
[[ $rawSelection == "" ]] && exit

# Sanitize selection
local files=()
for line in ${(f)rawSelection}; do
  local split=(${(@s/   /)line})
  
  local filepath="$split[1]"
  filepath=${filepath:q}
  filepath=${~filepath}

  files+=($filepath)
done

echo $files

