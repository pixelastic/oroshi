#!/usr/bin/env zsh
# Returns the top elements of a json array file
# Usage:
# $ json-head file.json 10 # Output the first 10 elements of the file

local inputFile="${1:a}"
local headLimit="$2"

if [[ $headLimit == "" ]]; then
  echo "âœ˜ You must pass the number of elements to return"
  exit 1
fi

jq \
  ".[:${headLimit}]" \
  $inputFile
