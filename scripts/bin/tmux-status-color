#!/usr/bin/env zsh
# Return the color for a specific name
# Usage:
#   tmux-status-color projects
local name="$1"
local color=""

[[ $name == "bg" ]] && color='colour24'
[[ $name == "perso" ]] && color='colour141'
[[ $name == "projects" ]] && color='colour35'
[[ $name == "norska" ]] && color='colour69'
[[ $name == "golgoth" ]] && color='colour130'
[[ $name == "aberlaas" ]] && color='colour136'
[[ $name == "renovate" ]] && color='colour33'
[[ $name == "tmp" ]] && color='colour233'
[[ $name == "indexing" ]] && color='colour27'
[[ $name == "monsters" ]] && color='colour124'
[[ $name == "doctolib" ]] && color='colour33'
[[ $name == "on-circle" ]] && color='colour168'
[[ $name == "reddinx" ]] && color='colour111'
[[ $name == "crafting" ]] && color='colour94'
[[ $name == "painting" ]] && color='colour251'
[[ $name == "maps" ]] && color='colour31'
[[ $name == "barrowmaze" ]] && color='colour94'
[[ $name == "netlify-minutes" ]] && color='colour29'
[[ $name == "videogames" ]] && color='colour27'
[[ $name == "videogames-helper" ]] && color='colour27'
echo -n $color
