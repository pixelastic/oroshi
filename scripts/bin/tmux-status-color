#!/usr/bin/env zsh
# Return the color for a specific name
# Usage:
#   tmux-status-color projects
source ~/.oroshi/config/zsh/theming/colors.zsh
source ~/.oroshi/config/zsh/theming/projects.zsh

local name="$1"

local colorName=$PROJECTS[${name},background]
local colorCode=$COLOR[$colorName]

[[ $colorCode != '' ]] && echo "colour${colorCode}" && return
