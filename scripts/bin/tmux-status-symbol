#!/usr/bin/env zsh
# Return the symbol for a specific name
# Usage:
#   tmux-status-symbol projects
local name="$1"
local symbol=""

[[ $name == 'aberlaas' ]] && symbol=' '
[[ $name == 'aoinan' ]]   && symbol='  '
[[ $name == 'bg' ]]       && symbol='力'
[[ $name == 'books' ]]    && symbol=' '
[[ $name == 'comics' ]]   && symbol='X'
[[ $name == 'dropbox' ]]  && symbol=''
[[ $name == 'firost' ]]   && symbol='❯ '
[[ $name == 'golgoth' ]]  && symbol=' '
[[ $name == 'movies' ]]   && symbol=' '
[[ $name == 'music' ]]    && symbol=' '
[[ $name == 'norska' ]]   && symbol='煮'
[[ $name == 'oroshi' ]]   && symbol='x '
[[ $name == 'perso' ]]    && symbol=''
[[ $name == 'pictures' ]] && symbol=' '
[[ $name == 'projects' ]] && symbol='器'
[[ $name == 'reddinx' ]]  && symbol=' '
[[ $name == 'roleplay' ]] && symbol='ﲅ'
[[ $name == 'sov' ]]      && symbol='蠟'
[[ $name == 'tmp' ]]      && symbol='זּ '
[[ $name == 'twitter' ]]  && symbol=' '
[[ $name == 'videos' ]]   && symbol=' '
[[ $name == 'vim' ]]      && symbol=' '
[[ $name == 'www' ]]      && symbol=''
[[ $name == 'zsh' ]]      && symbol=' '
echo -n $symbol
