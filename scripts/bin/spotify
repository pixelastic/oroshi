#!/usr/bin/env sh

# Start spotify docked into the tray
# See
# https://stackoverflow.com/questions/10408816/how-do-i-use-the-nohup-command-without-getting-nohup-out
# for nohup syntax
nohup \
  kdocker \
    -qol \
    -d 15 \
    -i /home/tim/.oroshi/config/spotify/spotify.svg \
    /snap/bin/spotify \
    </dev/null >/dev/null 2>&1 &
