#!/usr/bin/env zsh
source ~/.oroshi/config/zsh/theming.zsh

local colorKeys=${(k)COLOR}
local sortedKeysAsString=("$(echo $colorKeys | tr ' ' '\n' | sort | tr '\n' ':')")
local sortedKeys=("${(@s/:/)sortedKeysAsString}")


for colorName in ${sortedKeys[@]}; do
  echo -n "${BG[$colorName]}          $RESET${FG[$colorName]}$RESET"
  echo -n "${FG[$colorName]}  ${colorName}$RESET"
  echo -n "${FG[$colorName]}  ${COLOR[$colorName]} $RESET"
  echo ""
done
