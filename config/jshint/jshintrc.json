{
  "bitwise": true,
  "browser": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "expr": true,
  "forin": true,
  "freeze": true,
  "funcscope": false,
  "globals": {
    "$": false,
    "_": true,
    "afterEach": false,
    "beforeEach": false,
    "ddescribe": false,
    "describe": false,
    "expect": false,
    "fixture": false,
    "iit": false,
    "it": false,
    "sinon": false,
    "xdescribe": false,
    "xit": false
  },
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxdepth": 3,
  "maxlen": 120,
  "maxparams": 3,
  "newcap": true,
  "noarg": true,
  "node": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "notypeof": false,
  "plusplus": false,
  "quotmark": "single",
  "strict": true,
  "undef": true,
  "unused": true
}
