# Parses the return of a fzf multi-selection
function fzf-fs-files-shared-postprocess() {
  local input="$@"

  # Stop early
  [[ $input == "" ]] && return

  # Sanitize selection
  local files=()
  for line in ${(f)input}; do
    local split=(${(@s/   /)line})

    local filepath="$split[1]"
    filepath=${filepath:q}
    filepath=${~filepath}

    files+=($filepath)
  done

  echo $files
}
