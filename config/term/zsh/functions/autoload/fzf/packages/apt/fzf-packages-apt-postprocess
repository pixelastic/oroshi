# Parses the return of a fzf multi-selection
function fzf-packages-apt-postprocess() {
  local rawSelection="$1"

  # Stop early
  [[ $rawSelection == "" ]] && exit

  # Sanitize selection
  local selections=()
  for line in ${(f)rawSelection}; do
    local split=(${(@s/â–®/)line})

    local selection="$split[1]"
    selections+=($selection)
  done

  echo ${=selections}
}
