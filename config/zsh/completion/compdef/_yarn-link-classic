#compdef

# Suggest Yarn Classic (v1) modules registered for linking
function _yarn-link-classic() {
  local suggestions=(${(f)"$(complete-yarn-link-classic)"})

  _describe -V "$(completion-header $COLOR_ALIAS_YARN_LINK_CLASSIC $COLOR_WHITE ' ï€® Yarn Classic (v1) registered modules for linking ')" suggestions
  return
}
