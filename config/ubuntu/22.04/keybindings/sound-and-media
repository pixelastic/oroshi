#!/usr/bin/env zsh

# Mute/Unmute microphone
kba "/org/gnome/settings-daemon/plugins/media-keys/mic-mute" "<Super>M"
# Next song
kba "/org/gnome/settings-daemon/plugins/media-keys/next" "<Super>L"
# Previous song
kba "/org/gnome/settings-daemon/plugins/media-keys/previous" "<Super>H"
# Play/Pause
kba "/org/gnome/settings-daemon/plugins/media-keys/play" "<Super>I"
