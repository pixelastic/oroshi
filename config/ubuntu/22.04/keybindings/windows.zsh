#!/usr/bin/env zsh

# Maximize
kba '/org/gnome/desktop/wm/keybindings/maximize' "<Super>Up"
# Minimize
kba '/org/gnome/desktop/wm/keybindings/unmaximize' "<Super>Down"
# Move to left
kba '/org/gnome/mutter/keybindings/toggle-tiled-left' "<Super>Left"
# Move to right
kba '/org/gnome/mutter/keybindings/toggle-tiled-right' "<Super>Right"
# Close
kba '/org/gnome/desktop/wm/keybindings/close' "<Alt>F4"
