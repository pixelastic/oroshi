#!/usr/bin/env zsh

# Alt-Tab
kba "/org/gnome/desktop/wm/keybindings/switch-windows" "<Alt>Tab"
# Show Desktop
kba "/org/gnome/desktop/wm/keybindings/show-desktop" "<Super>d"

# Move between monitors
kba "/org/gnome/desktop/wm/keybindings/move-to-monitor-down" "<Alt><Super>Down"
kba "/org/gnome/desktop/wm/keybindings/move-to-monitor-up" "<Alt><Super>Up"
