#!/usr/bin/env zsh

dconf write /org/gnome/desktop/wm/keybindings/move-to-monitor-down "['<Alt><Super>Down']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-monitor-up "['<Alt><Super>Up']"
dconf write /org/gnome/desktop/wm/keybindings/show-desktop "['<Super>d']"
dconf write /org/gnome/desktop/wm/keybindings/switch-windows "['<Alt>Tab']"
