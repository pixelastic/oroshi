#!/usr/bin/env zsh
# Keyboards and shortcuts > System
kba '/org/gnome/settings-daemon/plugins/media-keys/screensaver' "<Primary><Super>l"

kbz '/org/gnome/mutter/wayland/keybindings/restore-shortcuts'
kbz '/org/gnome/shell/keybindings/focus-active-notification'
kbz '/org/gnome/shell/keybindings/open-application-menu'
kbz '/org/gnome/shell/keybindings/toggle-application-view'
kbz '/org/gnome/shell/keybindings/toggle-message-tray'
kbz '/org/gnome/shell/keybindings/toggle-overview'
